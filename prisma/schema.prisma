datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Teacher {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  gradeLevel String
  subject    String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}